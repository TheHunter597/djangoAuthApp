{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "User",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "email": {
      "type": "string"
    },
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "avatar": {
      "type": "string"
    },
    "is_active": {
      "type": "boolean"
    }
  },
  "required": ["id", "first_name", "last_name", "avatar", "is_active", "email"]
}
