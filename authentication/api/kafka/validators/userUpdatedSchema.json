{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "User",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "data": {
      "type": "object",
      "properties": {
        "email": {
          "type": "string"
        },
        "first_name": {
          "type": "string"
        },
        "last_name": {
          "type": "string"
        },
        "avatar": {
          "type": "string"
        },
        "is_active": {
          "type": "boolean"
        }
      }
    }
  },
  "required": ["id", "data"]
}
